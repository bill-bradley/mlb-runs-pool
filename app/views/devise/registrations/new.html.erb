<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2>Sign up</h2>
      <hr/>

      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= devise_error_messages! %>
          <%= render partial: 'form', locals: {f:f} %>
          <%= f.submit "Sign up", class: 'btn btn-default btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
